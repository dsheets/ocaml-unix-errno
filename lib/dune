(library
  (name errno)
  (public_name unix-errno)
  (wrapped false)
  (modules errno)
  (libraries integers result)
  )

(library
  (name errno_host)
  (public_name unix-errno.host)
  (wrapped false)
  (modules :standard \ errno)
  (libraries errno integers result)
  )
